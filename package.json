{
  "name": "deno-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:vite": "vite",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./src/tailwind.css",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.4",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "babel-preset-solid": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-solid": "^2.3.10"
  }
}
